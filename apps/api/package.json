{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "build": "tsc --build",
    "postinstall": "npx puppeteer browsers install chrome || echo 'Chrome install failed, will try system path detection'",
    "install-chrome": "npx puppeteer browsers install chrome && echo 'Chrome installed successfully' || (echo 'Chrome install via puppeteer failed, trying system install...' && npm run install-chrome-system)",
    "install-chrome-system": "apt-get update && apt-get install -y google-chrome-stable || echo 'System Chrome install failed - will try runtime detection'",
    "clean": "rimraf dist .tsbuildinfo",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:reset": "tsx src/db/reset.ts",
    "db:view": "tsx src/db/view.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@clerk/backend": "latest",
    "@clerk/clerk-sdk-node": "latest",
    "@hono/node-server": "latest",
    "@hono/zod-validator": "latest",
    "@libsql/client": "latest",
    "@sparticuz/chromium": "^138.0.2",
    "axios": "latest",
    "bullmq": "^5.58.1",
    "chrome-launcher": "latest",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "glob": "^11.0.3",
    "hono": "4.9.4",
    "ioredis": "^5.7.0",
    "lighthouse": "latest",
    "mongoose": "latest",
    "node-cron": "latest",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "puppeteer": "^24.17.0",
    "puppeteer-core": "^24.17.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "winston": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "latest",
    "@types/node-cron": "latest",
    "@types/pg": "^8.15.5",
    "@types/puppeteer": "latest",
    "@types/swagger-ui-express": "latest",
    "@types/winston": "latest",
    "@types/yamljs": "latest",
    "drizzle-kit": "^0.31.4",
    "eslint": "latest",
    "rimraf": "^5.0.0",
    "shx": "^0.3.4",
    "swagger-ui-express": "latest",
    "tsx": "^4.20.5",
    "typescript": "latest",
    "yamljs": "latest"
  }
}