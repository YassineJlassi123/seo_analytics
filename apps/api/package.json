{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/app.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./client": "./dist/client.js"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.cjs",
    "db:reset": "tsx src/db/reset.ts",
    "db:view": "tsx src/db/view.ts",
    "db:studio": "drizzle-kit studio",
    "migrate": "tsx src/db/migrate.ts"
  },
  "dependencies": {
    "@clerk/backend": "latest",
    "@clerk/clerk-sdk-node": "latest",
    "@hono/node-server": "latest",
    "@hono/zod-validator": "latest",
    "@libsql/client": "latest",
    "axios": "latest",
    "bullmq": "^5.58.1",
    "chrome-launcher": "latest",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "hono": "4.9.4",
    "ioredis": "^5.7.0",
    "lighthouse": "latest",
    "mongoose": "latest",
    "node-cron": "latest",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "puppeteer": "^24.17.0",
    "winston": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "latest",
    "@types/node-cron": "latest",
    "@types/pg": "^8.15.5",
    "@types/puppeteer": "latest",
    "@types/swagger-ui-express": "latest",
    "@types/winston": "latest",
    "@types/yamljs": "latest",
    "drizzle-kit": "^0.31.4",
    "eslint": "latest",
    "swagger-ui-express": "latest",
    "tsx": "^4.20.5",
    "typescript": "latest",
    "yamljs": "latest"
  }
}